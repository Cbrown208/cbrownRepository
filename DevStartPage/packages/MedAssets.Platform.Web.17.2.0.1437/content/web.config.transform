<configuration>
	<configSections>
		<section name="aaaSecurity" type="MedAssets.Aaa.Security.Configuration.AaaSecuritySection, MedAssets.Platform.Web" />
	</configSections>
	<aaaSecurity cookieName="####### Enter Cookie/App Name here #######" cookieExpires="1440" ignorePathExtensions=".css;.js;.gif;.jpg;.ico;.png;*/#######/*;*/#######" pathsWontExtendGlobalSession="*.globalSessionInfo;" />
	<!-- set appropriate pattern for ignorePathExtensions that match the need of your application -->
	<system.webServer>
		<!--<modules> 
			<add name="AaaSessionEndModule" type="MedAssets.Aaa.Security.AaaSessionEndModule, MedAssets.Platform.Web, Culture=neutral" />
		</modules>-->
		<handlers>
			<add name="WifSessionInfoHandler" verb="*" path="*.globalSessionInfo" type="MedAssets.Aaa.WifSessionInfoHandler, MedAssets.Platform.Web" resourceType="Unspecified" />
		</handlers>
	</system.webServer>
</configuration>
